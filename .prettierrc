{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "endOfLine": "auto",
  "jsxSingleQuote": true,

  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "^@tanstack/(.*)$",
    "^@apis/(.*)$",
    "^@hooks/(.*)$",
    "^@common/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^styles/(.*)$",
    "^[./]",
    "^types",
    "<THIRD_PARTY_MODULES>"
  ],
  "importOrderSortSpecifiers": true
}
